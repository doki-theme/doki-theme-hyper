{
  "name": "doki-theme-hyper",
  "version": "3.0.0",
  "description": "The Doki Theme for Hyper",
  "main": "build/index.js",
  "repository": "https://github.com/Unthrottled/doki-theme-hyper.git",
  "author": "Alex Simons <alex@unthrottled.io>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "eslint src --fix && prettier --write src",
    "clean": "rm -rf build/",
    "prepare": "yarn clean && (yarn build || true)",
    "build": "tsc",
    "build:ultimate": "PRODUCT=ultimate yarn build",
    "prebuild": "yarn buildThemes",
    "buildThemes": "ts-node ./buildSrc/BuildThemes.ts",
    "buildThemes:ultimate": "PRODUCT=ultimate ts-node ./buildSrc/BuildThemes.ts",
    "predev": "yarn buildThemes",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.7",
    "@types/react": "^16.9.26",
    "@types/redux": "^3.6.0",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "base64-img": "^1.0.4",
    "eslint": "^7.1.0",
    "prettier": "^2.0.5",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "react": "^16.13.1"
  },
  "files": [
    "build/",
    "assets",
    "README.md"
  ]
}
